{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "coverage": "npm test -- --coverage --watchAll=false || true",
    "test": "cross-env NODE_ENV=testing node_modules/.bin/jest",
    "test -CI": "jest --watch=all",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|unimodules-permissions-interface|react-clone-referenced-element|@react-native-community|expo(nent)|expo|@expo|react-navigation|@react-navigation/.*|@unimodules/.*|sentry-expo|native-base)"
    ]
  },
  "dependencies": {
    "@kcodev/react-native-progress-bar": "^1.0.1",
    "@react-native-community/masked-view": "0.1.5",
    "@react-navigation/drawer": "^5.3.2",
    "@react-navigation/native": "^5.1.1",
    "@react-navigation/stack": "^5.2.3",
    "@types/react": "^16.9.25",
    "apollo-boost": "^0.4.7",
    "apollo-cache-persist": "^0.1.1",
    "expo": "~36.0.0",
    "graphql": "^14.6.0",
    "react": "~16.9.0",
    "react-apollo": "^3.1.3",
    "react-apollo-hooks": "^0.5.0",
    "react-dom": "~16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-progress": "^4.0.3",
    "react-native-reanimated": "~1.4.0",
    "react-native-safe-area-context": "0.6.0",
    "react-native-screens": "2.0.0-alpha.12",
    "react-native-web": "~0.11.7",
    "react-test-renderer": "^16.13.1",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "babel-preset-expo": "~8.0.0",
    "cross-env": "^7.0.2",
    "jest": "^25.1.0",
    "jest-expo": "^36.0.1"
  },
  "private": true
}
